<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="mainPanel" fx:id="mainScreenPanel" prefHeight="750.0" prefWidth="900.0" stylesheets="@../css/mainStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
   <children>
      <Button fx:id="btnNewGame" layoutX="60.0" layoutY="40.0" mnemonicParsing="false" onAction="#removeFocusFromButton" onMouseClicked="#goToNewGamePanel" onMouseEntered="#enteredOnButton" onMousePressed="#pressedOnButton" onMouseReleased="#releasedOnButton" prefHeight="50.0" prefWidth="150.0" styleClass="menuButton" stylesheets="@../css/mainStyle.css" text="Nowa Gra" />
      <Button fx:id="btnLoadGame" layoutX="270.0" layoutY="40.0" mnemonicParsing="false" onAction="#removeFocusFromButton" onMouseClicked="#goToLoadPanel" onMouseEntered="#enteredOnButton" onMousePressed="#pressedOnButton" onMouseReleased="#releasedOnButton" prefHeight="50.0" prefWidth="150.0" styleClass="menuButton" stylesheets="@../css/mainStyle.css" text="Wczytaj grę" />
      <Button fx:id="btnScoreBoard" layoutX="480.0" layoutY="40.0" mnemonicParsing="false" onAction="#removeFocusFromButton" onMouseClicked="#goToScoreBoard" onMouseEntered="#enteredOnButton" onMousePressed="#pressedOnButton" onMouseReleased="#releasedOnButton" prefHeight="50.0" prefWidth="150.0" styleClass="menuButton" stylesheets="@../css/mainStyle.css" text="Tablica wyników" />
      <Button fx:id="btnOptions" layoutX="690.0" layoutY="40.0" mnemonicParsing="false" onAction="#removeFocusFromButton" onMouseClicked="#goToOptions" onMouseEntered="#enteredOnButton" onMousePressed="#pressedOnButton" onMouseReleased="#releasedOnButton" prefHeight="50.0" prefWidth="150.0" styleClass="menuButton" stylesheets="@../css/mainStyle.css" text="Opcje" />
      <Button fx:id="btnExitGame" layoutX="690.0" layoutY="650.0" mnemonicParsing="false" onAction="#removeFocusFromButton" onMouseClicked="#goToExitGame" onMouseEntered="#enteredOnButton" onMousePressed="#pressedOnButton" onMouseReleased="#releasedOnButton" prefHeight="50.0" prefWidth="160.0" styleClass="menuButton" stylesheets="@../css/mainStyle.css" text="Wyjdź z gry" />
      <Pane id="newGamePanel" fx:id="paneNewGame" layoutX="50.0" layoutY="175.0" prefHeight="426.0" prefWidth="800.0" visible="false">
         <children>
            <Text fx:id="txtSelectShip" layoutX="317.0" layoutY="13.0" scaleX="3.0" scaleY="3.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wybierz statek:" x="40.0" y="40.0" />
            <Button fx:id="btnNextShip" layoutX="750.0" layoutY="163.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="30.0" text="&gt;" />
            <Button fx:id="btnPreviousShip" layoutX="20.0" layoutY="163.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="30.0" text="&lt;" />
            <Button fx:id="btnStartGame" layoutX="645.0" layoutY="375.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Rozpocznij grę" />
         </children></Pane>
      <Pane id="loadGamePanel" fx:id="paneLoadGame" layoutX="50.0" layoutY="175.0" prefHeight="426.0" prefWidth="800.0" visible="false">
         <children>
            <Button layoutX="645.0" layoutY="375.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" text="Usuń zapis" />
            <Button layoutX="450.0" layoutY="375.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Wczytaj grę" />
         </children></Pane>
      <Pane id="scoreBoardPanel" fx:id="paneScoreBoard" layoutX="50.0" layoutY="175.0" prefHeight="426.0" prefWidth="800.0" visible="false" />
      <Pane id="optionsPanel" fx:id="paneOptions" layoutX="50.0" layoutY="175.0" prefHeight="426.0" prefWidth="800.0" visible="false" />
   </children>
</AnchorPane>
